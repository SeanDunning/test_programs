#include <iostream>

using namespace std;

int main()
{

  for (int i =1; i <= 1000; i++)
  {
      cout << "\n";
      if (i % 3 != 0 && i % 5 != 0)
      {
          cout << i;
      }
      else
      {
          if (i % 3 == 0)
            cout << "Fizz";
          if (i % 5 == 0)
            cout << "Buzz";
      }
  }
 return 0;
}
